<div class="users_list">

    <h1>All Users</h1>
    <h3>{{ usersCount }} users</h3>
    <ul>
        {% for user in users %}
            <li>
                <h4>{{ user.username }}</h4>
                <p>{{ user.firstname }} {{ user.name }}</p>
                <a href="{{ url('/admin/users/edit', [user.id]) }}">Edit</a>
                <a href="{{ url('/admin/users/delete', [user.id]) }}">Delete</a>
            </li>
        {% endfor %}
    </ul>
</div>
